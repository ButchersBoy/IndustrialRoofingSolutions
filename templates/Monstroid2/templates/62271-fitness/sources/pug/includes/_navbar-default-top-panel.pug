// RD Navbar Top Panel
.rd-navbar-top-panel
  .rd-navbar-top-panel__main
    .rd-navbar-top-panel__content
      .rd-navbar-top-panel__left: p Premium HTML Theme
      .rd-navbar-top-panel__right: ul.rd-navbar-items-list
        //li: ul.list-inline-xxs
          li: a(href='#', data-toggle='modal', data-target='#modalLogin') Sign In
          li: a(href='#', data-toggle='modal', data-target='#modalRegister') Create an Account

        li: ul.list-inline-xxs
          li: a.icon.icon-xxs.fa.fa-facebook(href='#')
          li: a.icon.icon-xxs.fa.fa-twitter(href='#')
          li: a.icon.icon-xxs.fa.fa-google-plus(href='#')
          li: a.icon.icon-xxs.fa.fa-vimeo(href='#')
          li: a.icon.icon-xxs.fa.fa-youtube(href='#')
          li: a.icon.icon-xxs.fa.fa-pinterest-p(href='#')

  .rd-navbar-top-panel__aside
    ul.rd-navbar-items-list
      li: .rd-navbar-fixed__element-2: a.icon.icon-md.linear-icon-cart.link-gray-4(href='#')

  include _navbar-search