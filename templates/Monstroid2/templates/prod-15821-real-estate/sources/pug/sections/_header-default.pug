header.page-header
  // RD Navbar
  .rd-navbar-wrap
    nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-sm-device-layout='rd-navbar-fixed', data-md-layout='rd-navbar-fixed', data-md-device-layout='rd-navbar-fixed', data-lg-device-layout='rd-navbar-fixed', data-xl-device-layout='rd-navbar-static' data-xxl-device-layout='rd-navbar-static' data-lg-layout='rd-navbar-static' data-xl-layout='rd-navbar-static' data-xxl-layout='rd-navbar-static', data-stick-up-clone='false', data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true', data-md-stick-up-offset='150px', data-lg-stick-up-offset='50px')
      // RD Navbar Top Panel
      .rd-navbar-top-panel__outer 
        .rd-navbar-top-panel
          .rd-navbar-top-panel__main
            .rd-navbar-top-panel__toggle.rd-navbar-fixed__element-1.rd-navbar-static--hidden(data-rd-navbar-toggle='.rd-navbar-top-panel__main'): span
            .rd-navbar-top-panel__content
              ul.rd-navbar-items-list
                li: .unit.flex-row.unit-spacing-xs
                  .unit-left: span.icon.icon-xxs.icon-black.material-icons-location_on
                  .unit-body: a(href='#') 410 S Missouri St, Indianapolis, IN 46225, USA
  
                li: .unit.flex-row.unit-spacing-xs
                  .unit-left: span.icon.icon-xxs.icon-black.material-icons-phone
                  .unit-body: p Contact us directly at #[a(href='tel:#') +3(123) 789-5789]

      .rd-navbar-inner
        // RD Navbar Panel
        .rd-navbar-panel
          button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap'): span

          // RD Navbar Brand
          .rd-navbar-brand 
            a.brand__name(href='index.html')
              +png(logoDefault)
               
              

        // RD Navbar Nav
        .rd-navbar-nav-wrap 
          .rd-navbar-nav-wrap__element 
            a.button.button-primary-outline(href='contacts.html') REQUEST A QUOTE
          include ../includes/_nav