// Follow us
.sidebar-blog-item
	h5 Follow Us
	.post-classic-footer
		ul.list-inline.list-inline-lg
			li: a.icon.icon-sm.icon-secondary.icon-secondary-divider.fa.fa-facebook(href='#')
			li: a.icon.icon-sm.icon-secondary.icon-secondary-divider.fa.fa-twitter(href='#')
			li: a.icon.icon-sm.icon-secondary.icon-secondary-divider.fa.fa-google-plus(href='#')

	h5 Newsletter
	// RD Mailform
	form.rd-mailform.rd-mailform-blog.text-center(data-form-output="form-output-global", data-form-type='subscribe', method='post', action='bat/rd-mailform.php')
		.form-wrap
			input#contact-email.form-input(type="email" name="email" data-constraints="@Email @Required")
			label.form-label(for="contact-email") Enter e-mail
		button(type="submit").button.button-gradient-intro Get
			span.button-gradient-intro__overlay