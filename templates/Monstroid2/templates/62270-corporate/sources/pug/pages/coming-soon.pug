extends  ../_skeleton

//- Includes
include ../elements/_ui-breadcrumbs
include ../elements/_ui-rd-parallax

block variables
	- var activeIndex = 2
	- var pageName = 'Coming soon'

block page
	section.bg-accent.text-center.bg-image.bg-image-bottom.fullwidth-page(style='background-image: url(images/slider-slide-2-1920x930.jpg);')
		.section-sm
			.container
				.row.justify-content-sm-center
					.col-md-10.col-xl-8
						h2 This Site is Coming Soon
						.countdown.countdown-default(data-type='until', data-time='31 Jan 2019 16:00', data-format='dhms')
						h5 Sign up to Receive Updates
						p Get both our special offers and our oral health maintenance tips directly to your inbox!
						a(href='index.html').button.button-gradient-intro-outline Go back
							span.button-gradient-intro__overlay
