extends  ../_skeleton

//- Includes
include ../elements/_ui-breadcrumbs
include ../elements/_ui-tag-list
include ../elements/_ui-list-inline-icon
include ../elements/_ui-slider-blog
include ../elements/_ui-pagination-classic

block variables
	- var activeIndex = 4
	- var pageName = 'No Sidebar'

block page
	// Page header
	include ../sections/_header-default

	+breadcrumbs({
		path: [[menu[activeIndex].title, menu[activeIndex].link], [pageName]]
	})

	section.section-sm.section-sm-custom.bg-gray-3
		.container
			.row.row-70.justify-content-lg-center
				.col-lg-7.col-xl-8
					include  ../includes/_block-posts

	// Modal
	include ../sections/_section-modal

	// Page Footer
	include ../sections/_footer-corporate