extends  ../_skeleton

//- Includes
include ../elements/_ui-pagination
include ../elements/_ui-material-parallax

block variables
	- var activeIndex = 1
	- var pageClass = ''
	- var buyHref = '//www.templatemonster.com/website-templates/monstroid2.html'
	- var indexHref = '../62267-default'

block title
	title Home

block page
	// Page Header
	include ../sections/_header-default

	// Swiper
	section#home.section.swiper-container.swiper-slider(data-loop='false' data-simulate-touch="false" data-autoplay='false')
		.swiper-wrapper
			.swiper-slide-video.swiper-slide(data-slide-bg='images/home-slider-01.jpg' )
				.swiper-slide-caption.text-lg-left
					.container.container-wide
						.row.justify-content-lg-between.justify-content-center
							.col-xl-4.col-lg-5.offset-xl-1
								.oveflow-wrapper
									h5.big.text-accent(data-caption-animate='fadeInUp' data-caption-delay='400') Brand New & Improved!
								.oveflow-wrapper
									h1(data-caption-animate='fadeInUp' data-caption-delay='500') Monstroid²
								p(data-caption-animate='fadeInUp' data-caption-delay='600') Monstroid² boasts clean and crispy design, bulletproof layout consistence and intuitive navigation.
								ul.group.text-xs-nowrap
									li
										a(href=indexHref data-caption-animate='fadeInUp' data-caption-delay='500').button.button-white Live demo

							.col-md-9.col-xl-7.col-lg-6.video-caption-wrapper
								.video-caption
									img(src="images/home-slider-04.png", alt="").video-caption-overlay
									// RD Video
									.rd-video.play-on-scroll(data-rd-video-path="video/monstroid"  class="rd-video-player")
										.rd-video-wrap
											.rd-video-preloader
											video
											.rd-video-controls
												// Play\Pause button
												a.rd-video-play-pause.mdi.mdi-play.icon.icon-lg(href='#')
												// Progress bar
												.rd-video-progress-bar
													.current(style='width: 0%;')
												.rd-video-time
													// currentTime
													span.rd-video-current-time 00:00
													|  -
													// Track duration
													span.rd-video-duration 02:49
												.rd-video-volume-wrap
													// Volume button
													a.rd-video-volume.mdi.mdi-volume-high.icon.icon-sm(href='#')
													.rd-video-volume-bar-wrap
														// Volume bar
														.rd-video-volume-bar.rd-video-volume-bar-vertical


			.swiper-slide(data-slide-bg='images/home-slider-02.jpg')
				.swiper-slide-caption
					.container.container-wide
						h2(data-caption-animate='fadeInUp' data-caption-delay='300') Create and Customize Your Own Website
						h3(data-caption-animate='fadeInUp' data-caption-delay='400') It has never been easier!
						ul.group.text-xs-nowrap
							li
								a(href=indexHref data-caption-animate='fadeInUp' data-caption-delay='500').button.button-white Live demo


			.swiper-slide(data-slide-bg='images/home-slider-03.jpg')
				.swiper-slide-caption
					.container.container-wide
						h2(data-caption-animate='fadeInUp' data-caption-delay='300') A Variety of Available Customization Options
						ul.group.text-xs-nowrap
							li
								a(href=indexHref data-caption-animate='fadeInUp' data-caption-delay='500').button.button-white Live demo

